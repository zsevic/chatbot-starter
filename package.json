{
  "name": "chatbot-starter",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "telegram-webhook:set": "bottender telegram webhook set -w $1",
    "start": "nest start",
    "start:dev": "NODE_ENV=development TZ=UTC nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_PATH=./dist/src NODE_ENV=production TZ=UTC node dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose --config ./test/jest-e2e.js"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/swagger": "^6.1.3",
    "body-parser": "^1.20.1",
    "bottender": "^1.5.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "mongoose": "^6.7.3",
    "morgan": "^1.10.0",
    "nest-winston": "^1.8.0",
    "ngrok": "^4.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "shelljs": "^0.8.5",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "tsconfig-paths": "^4.1.0"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format",
      "git add --force"
    ]
  }
}
